#!/bin/sh

mypath=`realpath $0`
directory=`dirname $mypath`
filename=`basename $directory`
cd $directory

#git config --global user.email "papierpourgithub@gmail.com"
#git remote add $filename https://BRICOMATA8@github.com/BRICOMATA8/$filename.git

git add -v .
git commit -m "commit"
git push -u -f $filename master
